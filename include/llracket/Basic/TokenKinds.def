#ifndef TOK
#define TOK(ID)
#endif
#ifndef PUNCTUATOR
#define PUNCTUATOR(ID, SP) TOK(ID)
#endif
#ifndef KEYWORD
#define KEYWORD(ID, FLAG) TOK(kw_##ID)
#endif

TOK(unknown)
TOK(eof)
TOK(integer_literal)

PUNCTUATOR(l_paren, "(")
PUNCTUATOR(r_paren, ")")
PUNCTUATOR(plus, "+")
PUNCTUATOR(minus, "-")

KEYWORD(READ, KEYALL)

#undef KEYWORD
#undef PUNCTUATOR
#undef TOK
